{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://electro-mart-store.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
